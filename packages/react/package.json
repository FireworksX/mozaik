{
  "name": "@mozaikjs/react",
  "version": "0.0.2",
  "description": "React Adapter for work with mozaikjs",
  "license": "MIT",
  "homepage": "https://github.com/FireworksX/mozaik/tree/main/packages/react",
  "repository": "",
  "bugs": "",
  "keywords": [
    "state",
    "predictable",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "flux",
    "elm"
  ],
  "authors": [
    "FireworksX"
  ],
  "main": "build/index.js",
  "unpkg": "build/index.umd.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "src",
    "types"
  ],
  "scripts": {
    "clean": "rimraf build",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\" \"**/*.md\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.{js,ts}\" \"**/*.md\"",
    "lint": "eslint --ext js,ts src test",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "build": "npm run clean && microbundle --jsx React.createElement",
    "dev": "microbundle -w --compress false --jsx React.createElement",
    "pretest": "npm run build",
    "examples:lint": "eslint --ext js,ts examples",
    "examples:test": "cross-env CI=true babel-node examples/testAll.js"
  },
  "peerDependencies": {
    "@mozaikjs/core": "^0.4.0",
    "react": "^16.8.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "cross-env": "^7.0.2",
    "glob": "^7.1.6",
    "jest": "^25.4.0",
    "microbundle": "^0.12.0-next.9",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3",
    "typings-tester": "^0.3.2"
  },
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "jest": {
    "testRegex": "(/test/.*\\.spec\\.[tj]s)$",
    "coverageProvider": "v8"
  },
  "sideEffects": false,
  "dependencies": {
    "@mozaikjs/core": "^0.4.0",
    "@types/react": "^17.0.3"
  }
}
